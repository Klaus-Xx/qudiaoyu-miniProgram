/* pages/match/macthDetails/enrollList/enrollList.wxss */
page{
  background-color: #FFF;
}
.item{
  border-bottom: 1px solid #E6E6E6;
  display: flex;
  align-items: center;
  padding: 20rpx;
}
.item image{
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
  border-radius: 12rpx;
}
.content{
  flex-grow: 1;
  font-size: 32rpx;
  color: #000;
}
.content span{
  display: block;
}

.label{
  padding-right: 20rpx;
  color: #666;
}

.item .cGreen{
  color: #259B24;
}
<!--pages/match/macthDetails/signUp/signUp.wxml-->
 <form  bindsubmit="sub">
  <view class="list">
  
      <view class="title">报名信息</view>
      <view class="item">
          姓名：<input name="name" placeholder='请输入您的姓名'></input>
      </view>
      <view class="item">
          电话：<input name="mobile" placeholder='请输入电话号码'></input>
      </view>
      <view class="item">
          身份证：<input name="cardNo" placeholder='请输入身份证号码'></input>
      </view>
  </view>
  <button formType="submit">提交报名</button>
</form>
<!--pages/match/macthDetails/signUp/signUp.wxml-->
 <form  bindsubmit="sub">
  <view class="list">
      <view class="title">报名信息</view>
      <view class="item">
          <view>姓名：</view><input name="name" placeholder='请输入您的姓名'></input>
      </view>
      <view class="item">
          <view>电话：</view><input name="mobile" placeholder='请输入电话号码'></input>
      </view>
      <view class="item">
          <view>身份证号：</view><input name="cardNo" placeholder='请输入身份证号码'></input>
      </view>
  </view>
  <button formType="submit">{{context}}</button>
</form>